      <data key="d2">e5545ffc2d2770c13bb5f00c9552d741</data>
    </node>
    <node id="TRANSPORTEURS SUCCESSIFS">
      <data key="d0">ROLE</data>
      <data key="d1">Les diff&#233;rents transporteurs impliqu&#233;s dans le processus de transport de marchandises par route, chacun prenant part &#224; une &#233;tape du transport</data>
      <data key="d2">e5545ffc2d2770c13bb5f00c9552d741</data>
    </node>
    <node id="R&#201;MUN&#201;RATION DU TRANSPORT">
