      <data key="d5">621a8bcde98b24680cece1ad771ba5d6</data>
    </edge>
    <edge source="TRANSPORTEUR" target="BRIS D'EMBALLAGE">
      <data key="d3">9.0</data>
      <data key="d4">Le transporteur prend des mesures en cas de bris d'emballage pour prot&#233;ger la marchandise</data>
      <data key="d5">621a8bcde98b24680cece1ad771ba5d6</data>
    </edge>
    <edge source="TRANSPORTEUR" target="GARDE DE LA MARCHANDISE">
      <data key="d3">9.0</data>
      <data key="d4">Le transporteur assume la garde de la marchandise apr&#232;s un incident</data>
      <data key="d5">621a8bcde98b24680cece1ad771ba5d6</data>
